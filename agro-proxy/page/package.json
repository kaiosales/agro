{
  "name": "agro",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "webpack --mode development --config webpack.config.js --watch",
    "build:prod": "webpack --mode production --config webpack.config.js",
    "build:css": "node-sass --include-path scss landing/scss/main.scss landing/css/main.css",
    "watch-css": "nodemon -e scss -x 'npm run build'"
  },
  "devDependencies": {
    "css-loader": "^4.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.0.9"
  }
}
